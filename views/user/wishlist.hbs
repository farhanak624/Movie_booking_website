<section>
    <div class="container pt-5">
    <div class="row">
      <div class="container mb-3">
        <h1 class="justify-content-center">My Wishlist</h1>
      </div>
      {{#each wishlistedMovies}}
      <div class=" col-lg-3 col-md-6 col-12 pt-4">
        <div class="card" style="">
          <div class="bg-image hover-zoom">
          <img src="/images/posters/{{this._id}}.jpg" style="height:400px;" class="card-img-top" alt="...">
          </div>
          <div class="card-body">
            <h5 class="card-title">{{this.movieWishList.movie}}</h5>
            <h6>{{this.movieWishList.genre}}</h6><h6>{{this.movieWishList.duration}}</h6>
            <p class="collapse mb-3 card-text">{{this.movieWishList.about}}</p>
            <a href="" class="btn btn-outline-success">Book my show</a>
          </div>
        </div>
      </div>
      {{/each}}
    </div>
  </div>

</section>